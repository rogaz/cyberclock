<%= provide(:title, 'Crear usuario') %>
<h1>Crear usuario</h1>

<%= form_for(@user, html: {class: 'form-horizontal', role: 'form'}) do |f| %>
<%= render 'form', f: f %>
<div class="form-group">
  <div class="col-sm-offset-2 col-sm-10">
    <%= f.submit class: 'btn btn-default' %>
  </div>
</div>
<% end %>

<%= link_to 'Back', users_path %>
